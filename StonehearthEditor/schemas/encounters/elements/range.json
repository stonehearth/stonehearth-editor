{
   "$schema": "http://json-schema.org/draft-04/schema#",
   "id": "http://stonehearth.net/schemas/encounters/elements/range.json",
   "title": "Range",
   "description": "A numerical range from a minimum to a maximum, usually inclusive.",
   "type": "object",
   "properties": {
      "min": {"type": "number"},
      "max": {"type": "number"}
   },
   "required": ["min", "max"],
   "default": "{\"min\": 0.0\", \"max\": 1.0}",
   "additionalProperties": false
}
